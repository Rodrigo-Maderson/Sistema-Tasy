          <DS_SQL><![CDATA[select	substr(obter_nome_pf(cd_pessoa_fisica),1,60) nm_medico,
	obter_medico_convenio(1,cd_pessoa_fisica,1) cd_medico_convenio,	
	substr(un_converte_hr_min_format(trunc(sum(obter_hora_entre_datas(dt_inicio, dt_fim)))),1,100)	qt_horas,
	(trunc(sum(obter_hora_entre_datas(dt_inicio, dt_fim)))/:NR_PERIODO) nro_plantoes,
	((trunc(sum(obter_hora_entre_datas(dt_inicio, dt_fim)))/:NR_PERIODO) * :nr_uts) total_reais
	
from	escala_diaria 
where	(nr_seq_escala = :nr_sequencia_p or :nr_sequencia_p = :0)
and	(trunc(dt_inicio) between :dt_inicial_p and :dt_final_p) 
and	cd_pessoa_fisica is not null
group by	substr(obter_nome_pf(cd_pessoa_fisica),1,60), obter_medico_convenio(1,cd_pessoa_fisica,1)
order by  1]]></DS_SQL>
